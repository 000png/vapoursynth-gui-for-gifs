{
    "plugins": {
        "haf": "import havsfunc as haf",
        "descale": "import descale as descale",
        "mvs": "import mvsfunc as mvs",
        "fun": "import G41Fun as fun"
    },
    
    "preprocessor": {
        "type": "QTGMC 30 fast",
        "args": "Preset=\"Fast\", FPSDivisor=2"
    },

    "denoise": {
