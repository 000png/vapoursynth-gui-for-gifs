        "args": {
            "d": 0,
            "a": 6,
            "s": 4,
            "h": 1.2,
            "channels": "YUV"       
        }
    },

    "sharpen": {
        "type": "FineSharp",
        "args": {
            "sstr": 1.00
        }
    }
}